[{"印记": "", "題跋類別": "作者款識", "作者": "陳居中", "位置": "本幅", "款識": "□定二年春。陳居中畫。", "書體": "篆書", "全文": ""}, {"印记": "", "題跋類別": "題跋", "作者": "清高宗", "位置": "本幅", "款識": "甲午（西元一七七四年）仲春月御題", "書體": "草書", "全文": "名筆方傳名馬奇。玉題錦贉稱裝池。圉人雖執長韁控。卓立還思千里馳。吉騮一例此權奇。洪豁逺於鹽澤池。畫肉中猶兼畫骨。肯敎韓幹獨名馳。甲午（西元一七七四年）仲春月御題。"}, {"印记": "", "題跋類別": "題跋", "作者": "清高宗", "位置": "引首", "款識": "", "書體": "行書", "全文": "仿彿西徠。"}, {"印记": "", "題跋類別": "題跋", "作者": "文徵明", "位置": "拖尾", "款識": "文徵明", "書體": "草書", "全文": "八尺昂藏骨相奇。產來原是渥洼池。遠人知是朝廷重。牽入長安不敢馳。文徵明。"}]