[{"印记": "", "題跋類別": "作者款識", "作者": "周天球", "位置": "本幅", "款識": "庚辰（西元一五八○年）閏月之望。坐雨南山山閣。元素過集。為塗此紙。尚須借妙手為補坡陀。庶免作者譏評耳。六止生周天球記。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "題跋", "作者": "黎民表", "位置": "本幅", "款識": "黎民表", "書體": "隸書", "全文": "芬蘭著雨猗猗潤。翠葉牽風裊裊長。不向繽紛雜蕭艾。移來虛室自生香。黎民表。"}, {"印记": "", "題跋類別": "題跋", "作者": "張應泰", "位置": "本幅", "款識": "張應泰", "書體": "行書", "全文": "蘭澤多芳草。經冬能自保。習習國風吟。馨香發華藻。張應泰。"}, {"印记": "", "題跋類別": "題跋", "作者": "王仲房", "位置": "本幅", "款識": "王仲房", "書體": "行書", "全文": "一自靈均別。叢生負國香。誰堪寄作佩。孤詠望三湘。王仲房。"}, {"印记": "", "題跋類別": "題跋", "作者": "李言恭", "位置": "本幅", "款識": "李言恭", "書體": "行書", "全文": "紉作三閭佩。秋風氣自寒。國香原不偶。可並艾蕭看。李言恭。"}, {"印记": "", "題跋類別": "題跋", "作者": "鄭九夏", "位置": "本幅", "款識": "鄭九夏", "書體": "行書", "全文": "蘭生在深谷。逾遠而彌芳。既作高人佩。還稱王者香。鄭九夏。"}, {"印记": "", "題跋類別": "題跋", "作者": "顧為樞", "位置": "本幅", "款識": "顧為樞", "書體": "行書", "全文": "不共臙脂鬥斲牡丹。水邊石畔墨班班。要留清氣還天地。那許時人著眼看。顧為樞。"}, {"印记": "", "題跋類別": "題跋", "作者": "詹景鳳", "位置": "本幅", "款識": "詹景鳳", "書體": "行書", "全文": "蘭生不擇徑。春澤最葳蕤。燕姬得夢罷。入室更芳菲。詹景鳳。"}, {"印记": "", "題跋類別": "題跋", "作者": "笪重光", "位置": "本幅", "款識": "江上笪在辛", "書體": "行書", "全文": "冉冉幾叢玉。盈盈百斛香。猗歟九秋色。欲贈不能將。江上笪在辛。"}, {"印记": "", "題跋類別": "題跋", "作者": "區大相", "位置": "本幅", "款識": "區大相", "書體": "楷書", "全文": "露葉風花寫一枝。美人紉佩為誰貽。也知王者香猶在。九畹應憐楚客思。區大相。"}, {"印记": "", "題跋類別": "題跋", "作者": "許穀", "位置": "本幅", "款識": "許穀", "書體": "楷書", "全文": "九畹漪漪（應作猗）色。難同眾草論。棲真自深谷。避俗豈當門。作佩幽香遠。援玨木雅調存。足音元不至。滋露保靈桹（應作根）。許穀。"}, {"印记": "", "題跋類別": "題跋", "作者": "鄔佐卿", "位置": "本幅", "款識": "鄔佐卿", "書體": "行書", "全文": "楚佩曾遺澧水傍。夢中春色憶瀟湘。芳心只為靈均死。莫向風前怨未香。鄔佐卿。"}, {"印记": "", "題跋類別": "題跋", "作者": "朱多炡", "位置": "本幅", "款識": "來相如", "書體": "行書", "全文": "幽蘭奕奕吐奇芳。風過深林泛遠香。大似清真古君子。閉門高譽不能藏。來相如。"}, {"印记": "", "題跋類別": "題跋", "作者": "鮑應鰲", "位置": "本幅", "款識": "鮑應鰲", "書體": "楷書", "全文": "芳蘭堪結佩。采采儗同心。鋤罷還開路。歌喧自動琴。鮑應鰲。"}, {"印记": "", "題跋類別": "題跋", "作者": "胡汝煥", "位置": "本幅", "款識": "汝煥", "書體": "行書", "全文": "豈直德所比。兼之味更長。但知金可斷。何論國為香。汝煥。"}, {"印记": "", "題跋類別": "題跋", "作者": "朱多火貴", "位置": "本幅", "款識": "朱多火貴", "書體": "行書", "全文": "露氣滋崇蘭。清香滿深谷。延佇匪自傷。聊以趁幽獨。朱多火貴。"}, {"印记": "", "題跋類別": "題跋", "作者": "方元熲", "位置": "本幅", "款識": "方元熲", "書體": "行書", "全文": "露下芳苞折紫英。夜中香靄逼人清。援琹欲鼓不成調。一片楚江空月明。方元熲。"}, {"印记": "", "題跋類別": "題跋", "作者": "方弘靜", "位置": "本幅", "款識": "定之", "書體": "楷書", "全文": "映庭含淺色。淑景媚蘭湯。日麗參差影。風和輕重香。定之。"}, {"印记": "", "題跋類別": "題跋", "作者": "查士標", "位置": "本幅", "款識": ""}]