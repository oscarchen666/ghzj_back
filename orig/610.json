[{"印记": "", "題跋類別": "題跋", "作者": "項元汴", "位置": "本幅", "款識": "", "書體": "", "全文": "（索字小號）"}, {"印记": "", "題跋類別": "作者款識", "作者": "趙孟頫", "位置": "拖尾", "款識": "右子貢見原憲圖。要見貧無諂。富無驕之意。乃為盡其能事耳。然二子同出夫子之門。以道德為悅。豈以貧富為嫌哉。子昂。", "書體": "", "全文": ""}, {"印记": "", "題跋類別": "題跋", "作者": "湯炳龍", "位置": "拖尾", "款識": "北村老民湯炳龍題", "書體": "行書", "全文": "一從辭宰粟。飢餓隱空山。貨殖門前客。先生不啟關。北村老民湯炳龍題。"}, {"印记": "", "題跋類別": "題跋", "作者": "", "位置": "拖尾", "款識": "雲石", "書體": "行書", "全文": "處以道。雖得天下不足為富也。何必但以貧為羨耶。雲石。"}, {"印记": "", "題跋類別": "題跋", "作者": "倪瓚", "位置": "拖尾", "款識": "倪瓚", "書體": "行書", "全文": "賜也貨殖憲也貧。憲貧非病衣已鶉。未若簞瓢顏氏子。陋巷所樂皆天真。倪瓚。"}, {"印记": "", "題跋類別": "題跋", "作者": "張雨", "位置": "拖尾", "款識": "張天雨觀", "書體": "行書", "全文": "坐甕牖下。見貧居而無病色。此畫史用意所在。張天雨觀。"}, {"印记": "", "題跋類別": "題跋", "作者": "顧瑛", "位置": "拖尾", "款識": "金粟道人顧阿瑛", "書體": "行書", "全文": "道之不行謂之病。財之不給謂之貧。賜也能言心未識。虛勞結駟踏清塵。金粟道人顧阿瑛。"}, {"印记": "", "題跋類別": "題跋", "作者": "姚綬", "位置": "拖尾", "款識": "成化己亥十月望姚綬書", "書體": "行書", "全文": "賜憲皆聖門高弟。重裀列鼎。甕牖繩樞。曰有命焉。富不為富所移。貧不為貧所困。皆可語於道也。若以貨殖病賜。則其晚聞性與天道者何如。因觀陶文式所藏趙伯駒圖一幅。兼誦前輩之作。末附此論。尚有待於知言者。成化己亥十月望姚綬書。"}, {"印记": "", "題跋類別": "題跋", "作者": "沈周", "位置": "拖尾", "款識": "沈周", "書體": "", "全文": "余嘗見錢舜舉作此圖。今又見是卷。運筆傅色人指謂松雪學士。非其手不能臻妙如此。豈舜舉當時亦愛此而仿為之耶。公綬先生鑒為趙伯駒。其直家製耳。至乎無諂無驕之意。又非胸中固有其象而後能言之。此是松雪度越人處。可不尚哉。沈周。"}, {"印记": "", "題跋類別": "題跋", "作者": "史鑑", "位置": "拖尾", "款識": "松陵史鑑觀", "書體": "楷書", "全文": "弘治六年歲次癸丑冬十有二月。松陵史鑑觀。"}, {"印记": "", "題跋類別": "題跋", "作者": "姚綬", "位置": "引首一", "款識": "賜進士中臺歸叟姚綬書", "書體": "行書", "全文": "甕牖圖。賜進士中臺歸叟姚綬書。"}, {"印记": "", "題跋類別": "題跋", "作者": "金湜", "位置": "引首二", "款識": "四明八十一叟金湜書", "書體": "隸書", "全文": "甕牖圖。四明八十一叟金湜書。"}, {"印记": "", "題跋類別": "題跋", "作者": "項元汴", "位置": "卷末", "款識": "", "書體": "", "全文": "明嘉靖卅年秋八月重裝于天籟閣。"}, {"印记": "", "題跋類別": "題跋", "作者": "項元汴", "位置": "卷末", "款識": "", "書體": "", "全文": "元趙松雪倣趙千里刷色畫原憲甕牖圖逸品。"}, {"印记": "", "題跋類別": "題跋", "作者": "項元汴", "位置": "卷末", "款識": "", "書體": "", "全文": "墨林項元汴珍祕。"}, {"印记": "", "題跋類別": "題跋", "作者": "項元汴", "位置": "卷末", "款識": "", "書體": "", "全文": "明崇禎甲戌年夏重裱于樂志堂。"}, {"印记": "", "題跋類別": "題跋", "作者": "項元汴", "位置": "卷末", "款識": "", "書體": "", "全文": "原價五十兩。"}]