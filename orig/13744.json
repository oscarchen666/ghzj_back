[{"印记": "", "題跋類別": "作者款識", "作者": "李公麟", "位置": "本幅", "款識": "李公麟畫", "書體": "楷書", "全文": ""}, {"印记": "", "題跋類別": "題跋", "作者": "", "位置": "本幅", "款識": "", "書體": "行楷書", "全文": "蘇子瞻。王定國。王晉卿。蔡天啟。蘇子由。黃魯直。張文潛。李伯時。元沖之。公素。王欽臣。米元章。秦少游。陳碧盧。劉巨濟。圓通秀老。（依人物各標名）。"}, {"印记": "", "題跋類別": "題跋", "作者": "", "位置": "本幅", "款識": "", "書體": "行楷書", "全文": "西園雅集圖"}, {"印记": "", "題跋類別": "題跋", "作者": "王穀祥", "位置": "引首", "款識": "酉室王穀祥題", "書體": "行書", "全文": "千古流風。（隸書）。酉室王穀祥題。"}, {"印记": "", "題跋類別": "題跋", "作者": "宋濂", "位置": "拖尾", "款識": "金華宋濂題", "書體": "草書", "全文": "三月韶華風景佳。西園雅集正堪誇。雲芝匝地連瑤草。琪樹交枝綴寶花。游賞一時皆俊彥。流傳千古見繁華。于今戚里恩榮美。百世高風總一家。金華宋濂題。"}, {"印记": "", "題跋類別": "題跋", "作者": "聶大年（姓名待考）", "位置": "拖尾", "款識": "聶大年識", "書體": "行楷書", "全文": "西園雅集為一時艷事。余曾見李伯時團扇小幅。後有米元章蠅頭小楷書題。精妙絕倫。今復見此白描卷。丰神儼然。呼之欲出。苟非名筆。其孰能至于斯。聶大年識。"}, {"印记": "", "題跋類別": "題跋", "作者": "文嘉", "位置": "拖尾", "款識": "", "書體": "楷書", "全文": "西園雅集圖記。（文不錄。）"}]