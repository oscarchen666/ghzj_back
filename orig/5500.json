[{"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "牡丹須綠葉。春早葉難多。莫道扶持少。春王捧絳羅。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "一斗湖光不放寬。卻看紙上定波瀾。犀盤黑盡渾無密。捧出茆山女道冠。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "菊花自古只黃花。改樣連年漸漸差。不是老夫嫌冷淡。敢將紅粉向儂搽。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "蘭與竹相並。非關調本同。氤氳香不遠。聊為引清風。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "虢姨騎馬去朝天。淡掃蛾眉真可憐。不信馬頭毯兩串。也如枝上粉團團。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "葉須犀甲厚。花放鶴頭丹。歲暮饒冰雪。朱顏不改觀。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "老夫一掃秋園卉。六片尖尖雪色流。用盡邢州沙萬斛。未便琢出此搔頭。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "五寸珊瑚珠一囊。東風吹老海榴黃。宵來酒渴真無奈。換取金刀劈玉漿。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "湘水拂鏡明。江波蹙鮮滑。湘君少侍兒。煩儂步羅襪。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "青鸞五尺尾。一半入青霄。老眼摩挲看。方知是竹梢。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "從來不見梅花譜。信手拈來自有神。不信試看千萬樹。東風吹著便成春。", "書體": "行書", "全文": ""}, {"印记": "", "題跋類別": "作者款識", "作者": "徐渭", "位置": "本幅", "款識": "青藤道人徐渭畫并題", "書體": "行書", "全文": ""}]